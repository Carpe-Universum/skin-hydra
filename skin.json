		"SkinPreloadExistence": [
			"HydraHooks::onSkinPreloadExistence"
		],
		"SkinTemplateOutputPageBeforeExec": [
			"HydraHooks::onSkinTemplateOutputPageBeforeExec"
		],
		"OutputPageBodyAttributes": [
			"HydraHooks::onOutputPageBodyAttributes"
		],
		"SkinVectorStyleModules": [
			"HydraHooks::onSkinVectorStyleModules"
		],
		"SkinMinervaDefaultModules": [
